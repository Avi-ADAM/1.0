.tiptap ol li {
  counter-increment: muffins;
}
.tiptap ol li:before {
  content: counter(muffins) ". ";
  color:var(--gold);
}
.tiptap ol {
  list-style: none;
  counter-reset: muffins;
}
.tiptap ul
{
      margin:0px 40px ;
    padding: 0 0 0 40px;
}
/*   //     list-style-image: linear-gradient(to left bottom, red, blue);

*/
.tiptap ul
{
    list-style-image: url('https://res.cloudinary.com/love1/image/upload/v1701972931/list_agpi7e.svg');
    display: list-item;
}
.tiptap a{
  color: var(--lturk);
}
   .tiptap h1{
  font-size: 1.5rem;
}
.tiptap h3{
  font-size: 1rem;
}
.tiptap p{
  font-size: 0.8rem;
}
@media(min-width: 450px){
    .tiptap h1{
  font-size: 2rem;
}
.tiptap h3{
  font-size: 1.5rem;
}
.tiptap p{
  font-size: 1.2rem;
}
}
.tiptap p, h1, h2, h3, a,li, ul {
  background-color: inherit;
  color: inherit;
}
[data-color="pink"] > ::before {
  content: "❌ ⟵ : ";
  color: darkred;
  font-weight: bold;
}

[data-color="#a7f2a4"] > ::before {
  content: " ✔ ⟵: ";
  color: darkgreen;
  font-weight: bold;
  
}

/* היילייט חיצוני */
[data-color="pink"] {
  border: 1px solid pink;
  padding: 5px;
  background-color: rgba(255, 182, 193, 0.2); /* ורוד חצי שקוף */
}

[data-color="#a7f2a4"] {
  border: 1px solid lightgreen;
  padding: 5px;
  background-color: #a7f2a4; /* ירוק חצי שקוף */
}
@media(min-width: 670px){
  .tiptap h1{
  font-size: 2.5rem;
}
.tiptap h3{
  font-size: 2rem;
}
.tiptap p{
  font-size: 1.5rem;
}
}
.tiptap blockquote {
    font-family: Sarabun;
  box-shadow: 7px 7px 13px lightgrey;
  background: var(--gold);
  margin-bottom:18px;
  padding: 15px;
  font-size: 22px;
  border-left: solid var(--mturk);
  border-right: solid var(--mturk);

}


.tiptap blockquote:before {  
    content: open-quote;  
    font-size: 24pt;  
    text-align: center;  
    line-height: 42px;  
    color: #fff;  
    background: #ddd;  
    float: left;  
    position: relative;  
    border-radius: 25px;  
   margin-right: 0.5em;
  vertical-align: -0.4em;
  
    /** define it as a block element **/  
    display: block;  
    height: 25px;  
    width: 25px;  
}  
